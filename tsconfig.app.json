    "outDir": "./out-tsc/app",
    "types": ["emscripten"],
    "paths": {
      "vscode": ["node_modules/monaco-languageclient/lib/vscode-compatibility"]
    },
    "skipLibCheck": true,
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
